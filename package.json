  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "classnames": "^2.2.6",
    "copy": "^0.3.2",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
